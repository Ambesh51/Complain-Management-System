  <body>
    <div class="container">


<form  id='myform' method="post" action ="/composeClg">
  <h4>Name</h4>
    <input type="text" class="form-control" name="postName" maxlength="30" placeholder="Name Not Mentioned"  required>
  <br>
<h4>Title</h4>
<input type="text" class="form-control" name="postTitle" required ><br>
<h4>Problem</h4>
<textarea  rows="5" cols="50"  class="form-control" name="postContent" required></textarea><br>
<select name="branch" id="">
    <option value="CSE">CSE</option>
    <option value="ME">ME</option>
    <option value="IT">IT</option>
    <option value="CIVIL">CIVIL</option>
    <option value="EE">EE</option>
    <option value="EC">EC</option>
  </select>


<select name="section" id="" style="margin-left: 30px">
    <option value="A">A</option>
    <option value="B">B</option>
    <option value="C">C</option>
    <option value="D">D</option>
  </select>

<br>
<br>
<input type="radio" name="year" value="1" required>
  <label for="1">1 st Year</label><br>
<input type="radio" name="year" value="2" required>
  <label for="2">2 nd Year</label><br>
  <input type="radio" name="year" value="3" required>
  <label for="3">3 rd Year</label><br>
<input type="radio" name="year" value="4" required>
  <label for="4">4 th Year</label><br><br>
  
      
<button type="submit" class="btn btn-primary">Proced</button>

</form>


